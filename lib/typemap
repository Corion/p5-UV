UV::Loop            T_PTROBJ

int64_t             T_INT64
uint64_t            T_UINT64

uv_handle_t *       T_PTROBJ_MG

INPUT

T_INT64
    $var = SvI64($arg);

T_UINT64
    $var = SvU64($arg);

T_PTROBJ_MG
    $var = ($type)xs_object_magic_get_struct_rv_pretty(aTHX_ $arg, \"$var\");

OUTPUT

T_INT64
    $arg = newSVi64($var);

T_UINT64
    $arg = newSVu64($var);
